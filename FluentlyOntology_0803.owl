        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isRelatedToConcept"/>
        <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Concept"/>
        <rdfs:range rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Concept"/>
        <rdfs:comment>Direct precedence applied to concepts. E.g. the role &apos;Executive&apos; is superordinated to &apos;DepartmentManager&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">is superordinated to</rdfs:label>
        <rdfs:label xml:lang="it"> superordinato a</rdfs:label>
    </owl:ObjectProperty>



    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#isTaskDefinedIn -->

    <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#isTaskDefinedIn">
