        <rdfs:range rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Situation"/>
        <rdfs:label xml:lang="en">is agent included in</rdfs:label>
        <rdfs:label xml:lang="it"> un agente nel contesto di</rdfs:label>
