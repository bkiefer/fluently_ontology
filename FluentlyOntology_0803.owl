        <rdfs:range rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Concept"/>
        <rdfs:comment>Direct succession applied to concepts. E.g. the role &apos;Officer&apos; is subordinated to &apos;Director&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">is subordinated to</rdfs:label>
        <rdfs:label xml:lang="it"> subordinato a</rdfs:label>
    </owl:ObjectProperty>



    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#isSuperordinatedTo -->

    <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#isSuperordinatedTo">
