        <owl:inverseOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isExpandedIn"/>
        <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Description"/>
