        <rdfs:label xml:lang="it">ha come obiettivo</rdfs:label>
        <rdfs:label xml:lang="en">has task</rdfs:label>
    </owl:ObjectProperty>



    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#includesAction -->

    <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#includesAction">
