        <rdfs:label xml:lang="it"> un ruolo di</rdfs:label>
    </owl:ObjectProperty>



    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#isSatisfiedBy -->

    <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#isSatisfiedBy">
